{
  "manifest_version": 2,
 
  "name": "SalesDriver Plugin",
  "description": "Tool for Quickly adding to the sales process",
  "version": "1.1.1.0",
  "browser_action": {
   "default_icon": "crewdriver-logo.png",
   "default_popup": "public/index.html"
  },
  "permissions": [
        "https://getworkers.agilecrm.com/dev/*",
        "tabs",
        "https://www.linkedin.com/sales/*"
  ],
  "background": {
        "scripts": ["background.js"],
        "persistent": false
  },
   "icons" : {
    "16": "crewdriver-logo.png"
   }
}